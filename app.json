{
  "expo": {
    "name": "ezzyapp",
    "slug": "ezzyapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/asili_no_bg.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ezzy.ezzyapp"
    },
    "android": {
      "package": "com.ezzy.ezzyapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logos/asili_no_bg.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/logos/asili_no_bg.png"
    },
    "extra": {
      "eas": {
        "projectId": "78738feb-1dca-486e-90d1-9e077bbb77a5"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true
          }
        }
      ]
    ]
  }
}